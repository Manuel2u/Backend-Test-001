{
  "name": "backend-test",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "cd": "graphql-codegen --config codegen.yml",
    "start": "node dist/src/index.js",
    "prestart": "yarn db",
    "db": "npx sequelize-cli db:migrate",
    "migration": "npx sequelize-cli migration:create --name",
    "dev": "yarn db && nodemon src/index.ts",
    "build": "sh build.sh",
    "predev": "cross-env NODE_ENV=development yarn db",
    "db:reset": "sequelize db:drop && sequelize db:create && sequelize db:migrate",
    "db:create:test": "cross-env NODE_ENV=test npx sequelize-cli db:create"
  },
